<% provide(:title, 'Nuevo Producto') %>

<h1>Nuevo Producto</h1>
<div class="row">
  <div class="col-md-4 col-md-offset-4 card">
    <%= form_for(@product, html: { multipart: true }) do |f| %>
      <%= render 'fields', f: f %>
      <%= f.submit "Agregar producto", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>
